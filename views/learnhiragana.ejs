<%- include header %>
<% if(data) { %>
<% for(var i = 0; i< data.length ; i++) {%>
<div class="modal" id=<%= data[i].word %>>
    <div class="animate container Container bg-light">
        <span onclick="document.getElementById('<%= data[i].word %>').style.display='none'" class="close"
            title="Close Modal">&times;</span>


        <form action="/learnhiragana" method="POST">
            <div class="row">
                <div style="-ms-flex: 80%;flex: 80%;margin-top: 5%;">
                    <div class="border border-dark rounded text-center" style="width: 400px;height: 400px;">
                        <% if(data[i].word.length==1){%>
                        <input type="hidden" name="word_remember" value=<%= data[i].sub %>>
                        <h1 style="font-size: 300px"><%= data[i].word %></h1>
                        <%} else {%>
                        <input type="hidden" name="word_remember" value=<%= data[i].sub %>>
                        <h2 style="font-size: 150px;margin-top: 75px"><%= data[i].word %></h2>
                        <%}%>
                    </div>
                </div>
                <div style="-ms-flex: 20%;flex: 20%;margin-top: 5%;">
                    <button class="btn-block btn btn-danger" type="button"
                        onclick="document.getElementById('<%= data[i].id %>').play()">Sound</button>
                        <button class="btn-block btn btn-info">Remember</button>
                    </div>
                </div>
        </form>

    </div>
</div>
<%}%>  <%}%>
